{
  "name": "rms-check-vscode",
  "displayName": "rms-check",
  "description": "Language server for linting and manipulating Age of Empires 2 Random Map Scripts.",
  "version": "0.0.0",
  "author": "Ren√©e Kooi <renee@kooi.me>",
  "activationEvents": [
    "onLanguage:aoe2-rms"
  ],
  "categories": [],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "rms-check",
      "properties": {
        "rmsCheck.trace.server": {
          "scope": "window",
          "type": "string",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "default": "off",
          "description": "Traces the communication between VS Code and the language server."
        }
      }
    }
  },
  "dependencies": {
    "vscode-jsonrpc": "^4.0.0",
    "vscode-languageclient": "^5.2.1",
    "which": "^1.3.1"
  },
  "devDependencies": {
    "browserify": "^16.5.0",
    "vsce": "^1.66.0",
    "vscode": "^1.1.34"
  },
  "extensionDependencies": [
    "deltaidea.aoe2-rms"
  ],
  "engines": {
    "vscode": "^1.23.0"
  },
  "keywords": [
    "Age of Empires 2",
    "aoe2",
    "multi-root ready",
    "random map script",
    "rms"
  ],
  "license": "MIT",
  "main": "./client/bundle",
  "publisher": "goto-bus-stop",
  "repository": {
    "type": "git",
    "url": "https://github.com/goto-bus-stop/rms-check",
    "directory": "vscode"
  },
  "scripts": {
    "build-client": "browserify --node -x vscode client/extension.js -o client/bundle.js",
    "build-server": "cd .. && wasm-pack build vscode/server --release --target nodejs",
    "prepare": "npm run build-server && npm run build-client",
    "package": "npm run prepare && vsce package"
  }
}
